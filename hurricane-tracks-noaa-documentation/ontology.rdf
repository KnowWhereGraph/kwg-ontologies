            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>NOAA Tropical Cyclone Segment Observation</rdfs:label>
    </owl:Class>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/NOAA_TropicalCycloneSegmentObservationCollection -->


