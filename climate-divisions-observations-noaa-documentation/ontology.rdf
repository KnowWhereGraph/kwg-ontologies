                <owl:allValuesFrom rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateObservableProperty"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This class denotes observation collections for a specific climate division, for a specific month.and a specific climate property.</rdfs:comment>
        <rdfs:label>Climate Division Monthly Observation Collection (Property Aggregated)</rdfs:label>
    </owl:Class>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivisionMonthlyResult -->


    <owl:Class rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivisionMonthlyResult">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:subClassOf rdf:resource="https://qudt.org/schema/qudt/QuantityValue"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://qudt.org/schema/qudt/hasUnit"/>
                <owl:allValuesFrom rdf:resource="https://qudt.org/schema/qudt/Unit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://qudt.org/schema/qudt/numericValue"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This class denotes measured climate results corresponding to an individual monthly observation measured at a climate division for a specific climate property.</rdfs:comment>
        <rdfs:label>Climate Division Monthly Observation Result</rdfs:label>
    </owl:Class>
    


