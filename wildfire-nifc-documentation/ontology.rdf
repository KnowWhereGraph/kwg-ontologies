    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/fireCause -->


    <owl:ObjectProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/fireCause">
        <rdfs:comment>This property is used to indicate the reason the fire occurred.</rdfs:comment>
        <rdfs:label>fire cause</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/fireDiscoveryTime -->


    <owl:ObjectProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/fireDiscoveryTime">
        <rdfs:comment>This property is used to indicate the date and time a fire was reported as discovered or confirmed to exist. May also be the start date for reporting purposes.</rdfs:comment>
        <rdfs:label>fire discovery time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/fireOutTime -->


    <owl:ObjectProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/fireOutTime">
        <rdfs:comment>This property is used to indicate the date and time when a fire is declared out.</rdfs:comment>
        <rdfs:label>fire out time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/firePointOfOrigin -->


    <owl:ObjectProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/firePointOfOrigin">
        <rdfs:comment>This property is used to indicate the point of origin of a fire incident.</rdfs:comment>
        <rdfs:label>fire point of origin</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/fireResponseTime -->


    <owl:ObjectProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/fireResponseTime">
        <rdfs:comment>This property is used to indicate the date/time of the initial response to the incident (when the IC arrives and performs initial size up)</rdfs:comment>
        <rdfs:label>fire response time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/hasTemporalScope -->


    <owl:ObjectProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/hasTemporalScope"/>
    


