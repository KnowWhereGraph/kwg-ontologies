    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/countyName -->


    <owl:DatatypeProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/countyName">
        <rdfs:label>county name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/dispatchCenterID -->


    <owl:DatatypeProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/dispatchCenterID">
        <rdfs:comment>A unique identifier for the dispatch center that intersects with the incident point of origin.</rdfs:comment>
        <rdfs:label>dispatch center ID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/gacc -->


    <owl:DatatypeProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/gacc">
        <rdfs:comment>A code that identifies the wildland fire geographic area coordination center (GACC) at the point of origin for the incident. A GACC is a facility used for the coordination of agency or jurisdictional resources in support of one or more incidents within a geographic area.</rdfs:comment>
        <rdfs:label>geographic area coordination center</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/hasFIPS -->


    <owl:DatatypeProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/hasFIPS"/>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/incidentName -->


    <owl:DatatypeProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/incidentName">
        <rdfs:label>incident name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/irwinID -->


    <owl:DatatypeProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/irwinID">
        <rdfs:label>irwin ID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/protectingAgency -->


    <owl:DatatypeProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/protectingAgency">
        <rdfs:comment>Indicates the agency that has protection responsibility at the point of origin of a fire incident.</rdfs:comment>
        <rdfs:label>protection agency</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/protectingUnit -->


    <owl:DatatypeProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/protectingUnit">
        <rdfs:comment>NWCG Unit responsible for providing direct incident management and services to an incident pursuant in its jurisdictional responsibility or as specified by law, contract or agreement. Protection can be re-assigned by agreement. The nature and extent of the incident determines protection (for example Wildfire vs. All Hazard.)</rdfs:comment>
        <rdfs:label>protection unit</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/stateName -->


    <owl:DatatypeProperty rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/stateName">
        <rdfs:label>state name</rdfs:label>
    </owl:DatatypeProperty>
    


