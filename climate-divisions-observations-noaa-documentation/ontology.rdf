    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivision -->


    <owl:Class rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivision">
        <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/spatialRelation"/>
                <owl:someValuesFrom rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/AdministrativeRegion_2"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/spatialRelation"/>
                <owl:someValuesFrom rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/S2Cell_Level13"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/hasDefaultGeometry"/>
                <owl:allValuesFrom rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/Geometry"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opengis.net/ont/geosparql#hasGeometry"/>
                <owl:allValuesFrom rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/Geometry"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This class denotes climate divisions in the NOAA CONUS climate divisional dataset.

Climate division used to generate climate analyses for the contiguous United States (CONUS).</rdfs:comment>
        <rdfs:label>Climate Division</rdfs:label>
    </owl:Class>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivisionMonthlyObservation -->


    <owl:Class rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivisionMonthlyObservation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/hasResult"/>
                <owl:allValuesFrom rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivisionMonthlyResult"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/phenomenonTime"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2006/time#DateTimeDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This class denotes monthly observations measured at a climate division for a specific climate property.</rdfs:comment>
        <rdfs:label>Climate Division Monthly Observation</rdfs:label>
    </owl:Class>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivisionMonthlyObservationCollection -->


    <owl:Class rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivisionMonthlyObservationCollection">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservationCollection"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/hasFeatureOfInterest"/>
                <owl:allValuesFrom rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivision"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/hasMember"/>
                <owl:allValuesFrom rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivisionMonthlyObservationCollection_Property"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This class denotes observation collection for a specific climate division and for a specific month.</rdfs:comment>
        <rdfs:label>Climate Division Monthly Observation Collection</rdfs:label>
    </owl:Class>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivisionMonthlyObservationCollection_Property -->


    <owl:Class rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivisionMonthlyObservationCollection_Property">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservationCollection"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/hasMember"/>
                <owl:allValuesFrom rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/ClimateDivisionMonthlyObservation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
