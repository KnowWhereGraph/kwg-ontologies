    <owl:NamedIndividual rdf:about="http://stko-kwg.geog.ucsb.edu/lod/resource/nifcFireObservableProperty.estimatedDollarCost">
        <rdf:type rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/NIFC_FireObservableProperty"/>
