    </owl:Class>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/ontology/NOAA_TropicalDepression -->


    <owl:Class rdf:about="http://stko-kwg.geog.ucsb.edu/lod/ontology/NOAA_TropicalDepression">
