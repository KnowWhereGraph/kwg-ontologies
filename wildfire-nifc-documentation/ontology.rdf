    <owl:Restriction>
        <owl:onProperty rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/fireDiscoveryTime"/>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2006/time#Instant"/>
        <rdfs:subClassOf rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/NIFC_Fire"/>
    </owl:Restriction>
    <owl:Restriction>
        <owl:onProperty rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/fireOutTime"/>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2006/time#Instant"/>
        <rdfs:subClassOf rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/NIFC_Fire"/>
    </owl:Restriction>
