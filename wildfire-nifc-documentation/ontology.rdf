    <!-- http://stko-kwg.geog.ucsb.edu/lod/resource/nifcFireObservableProperty.discoveryAcresBurnt -->


    <owl:NamedIndividual rdf:about="http://stko-kwg.geog.ucsb.edu/lod/resource/nifcFireObservableProperty.discoveryAcresBurnt">
        <rdf:type rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/NIFC_FireObservableProperty"/>
        <rdfs:comment>An estimate of acres burning when the fire is first reported by the first person to call in the fire. The estimate should include number of acres within the current perimeter of a specific, individual incident, including unburned and unburnable islands.</rdfs:comment>
        <rdfs:label>discovery acres burnt</rdfs:label>
    </owl:NamedIndividual>
    


