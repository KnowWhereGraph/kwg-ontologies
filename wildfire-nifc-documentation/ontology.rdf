        <rdfs:label>esitmated dollar cost</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://stko-kwg.geog.ucsb.edu/lod/resource/nifcFireObservableProperty.finalAcresBurnt -->


    <owl:NamedIndividual rdf:about="http://stko-kwg.geog.ucsb.edu/lod/resource/nifcFireObservableProperty.finalAcresBurnt">
        <rdf:type rdf:resource="http://stko-kwg.geog.ucsb.edu/lod/ontology/NIFC_FireObservableProperty"/>
        <rdfs:comment>Reported final acreage of incident.</rdfs:comment>
        <rdfs:label>final acres burnt</rdfs:label>
    </owl:NamedIndividual>
    


